package labguide7;

public class LabGuide7 {

    //Q1
    
    public static void main(String[] args) {
        
        int[] arr= {43, 91, 82, 456, 11, 10, 34, 76};
        
        System.out.println("The array has the following numbers: ");
        
        for(int i = 0; i < 8; i++){
            System.out.print(arr[i] + " ");
        }
        
    }
    
}
