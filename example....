#include <stdio.h>
#include <stdlib.h>

int main (void)
{
	double num1,num2;
	
	printf("Enter a number: ");
	scanf("%lf",&num1);
	
	printf("\nEnter a number: ");
	scanf("%lf",&num2);
	
	if (num1<num2) 
	
		printf("\nThe small number is %f",num1);

	else
		printf("\nThe small number is %f",num2);	

return 0;	
}
