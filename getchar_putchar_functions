int main(){
	
	char karakter;
	//klavyeden bir karakter oku ve enter tusunu bekle
	
	printf("bir karakter giriniz ve enter tusuna basiniz: ");
	karakter = getchar();
	printf("girilen karakter:");
	//okunan karakteri ekrana yazdir
	putchar(karakter);
	

