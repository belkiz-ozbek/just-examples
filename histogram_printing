#include<stdio.h>
#define SIZE 10

int main(){
	
	int n[SIZE] = {19, 3, 15, 7, 8, 9, 0, 1, 25, 4};
	int i, j;
	
	printf("%s%13s%17s\n", "Elemet", "Value", "Histogram");
	
	for(i = 0; i < SIZE; i++){
		printf("%7d%13d        ", i, n[i]);
		
		for(j = 0; j < n[i]; j++){
			printf("%c", '*');
		}
		printf("\n");
	}
	
	return 0;
}
