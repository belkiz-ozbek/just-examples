        //MultiDimensionalArray
        int[][] tablo = new int[][]{
            {1, 75, 10},
            {2, 87, 13},
            {3, 95, 14},
            {4, 102, 16},
            {5, 112, 19}
        };
        
        //tabloyu yazdÄ±r.
        for(int i = 0; i < tablo.length; i++){
            for(int j = 0; j < 3; j++){
                System.out.print(tablo[i][j] + " ");
            }
            System.out.println();
        }
        

    }

