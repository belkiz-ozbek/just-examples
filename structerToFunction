#include<stdio.h>

struct ogrenci{
	char isim[15];
	char soyisim[15];
	int nummara;
};

void goster(struct ogrenci a){
	printf("Ogrencinin bilgileri: %s %s %d", a.isim, a.soyisim, a.nummara);
}

int main(){
	
	struct ogrenci ogrenci1 = { "Belkiz", "Ozbek", 366475};
	goster(ogrenci1);
	
	return 0;
}
