#include<stdio.h>

int func(int num1, int num2){
	
	if(num2 % num1 == 0){
		return 1;
	}
	
	else{
		return 0;
	}
	
}	

int main(void){
	
	int num1, num2;
	
	printf("Enter two number: ");
	scanf("%d%d", &num1, &num2);
	
	printf("%d", func(num1, num2));
	
	return 0;
}


OR +%&/()(='!^'^+%/)=^%&^&%/(()=)=!+r%t&/( //BU BİR BÖLME ÇİZGİSİDİR!!!!!!

#include<stdio.h>
	

int main(void){
	
	int num1, num2;
	
	printf("Enter two number: ");
	scanf("%d%d", &num1, &num2);
	
	if(num2 % num1 == 0){
		printf("1");
	}
	
	else{
		printf("0");
	}
	
	return 0;
}
