#include<stdio.h>

int func(int num1, int num2){
	
	return(num2 % num1 == 0); 
	
}	

int main(void){
	
	int num1, num2;
	
	printf("Enter two number: ");
	scanf("%d%d", &num1, &num2);
	
	printf("%d", func(num1, num2));
	
	return 0;
}


